{
   
    "version": 2,
    "builds": [
        {
            "src": "HealHaven/manage.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.11", "maxLambdaSize": "50mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "functions": {
        "api/*": {
            "maxDuration": 30
        }
    }
}
